<%# console.log("Item:", key, value) %>
<div class="row">
  <% if (typeof value === 'object') { %>
    <button type="button" id="<%= popoverID %>" class="btn btn-secondary" 
      data-toggle="popover" title="<%= key%>" data-content="...",
      class="popover-object-button"> 
      <%= key%> 
    </button>  
  <% } else { %>
  <div class="col-6">
    <%= key %>
  </div>
  <div class="col-6">
    <%= value %>
  </div>
  <% } %>
</div>

<script>
  $('#<%=popoverID%>').popover({
    html: true
  });
  $('#<%= popoverID %>').on('shown.bs.popover', function() {
    <% printObject() %>;
  });
</script>