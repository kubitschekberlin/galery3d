<%# console.log("Item:", key, value) %>
<div class="row">
  <div class="col-6">
    <%= key %>
  </div>
  <div class="col-6">
  <% if (typeof value === 'object') { %>
    <button type="button" id="button_<%=popoverID%>" 
      class="btn btn-secondary popover-object-button" 
      data-key="<%=key%>">Show</button>  
  <% } else { %>
    <%=value%>
    <% } %>
  </div>
</div>
<script>
  <% storeObject('#button_' + popoverID, value); %>
</script>