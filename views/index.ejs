<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <link rel="stylesheet" href="galery_3d.css">
  <link rel="stylesheet" href="./css/bootstrap.css">
  <link rel="stylesheet" href="./css/jquery-ui.css">
</head>
<body>
  <div id="top_area" class="container-fluid top-area">
    <div class="row top-area">
      <div class="col-auto">
        <h2><%= title %></h2>
        <form class="mt-3">
          <input class="form-control" type="file" id="formFile" accept=".stl, .obj, .ply">
        </form>
        <div>
          <div class="form-check mt-2">
            <input class="form-check-input" type="checkbox" value="" id="object_properties_button">
            <label class="form-check-label" for="object_properties_button">
              Show Properties
            </label>
          </div>  
          <div id="object_properties">
          </div>     
        </div>
        <div>
          <input class="btn btn-secondary js-reset mt-2" type="button" value="Reset Object/Camera" />
        </div>
      </div>
      <div class="col" id="canvas_parent"></div>
    </div>
  </div>
  <script src="./dist/client.bundle.js"></script>
</body>
</html>
